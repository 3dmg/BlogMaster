<?xml version="1.0" encoding="utf-8"?>
<manifest package="at.mg.blogmaster" android:versionCode="1"
	android:versionName="0.1" android:installLocation="auto"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<uses-sdk android:minSdkVersion="4" />


	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:name="BlogApp" android:permission="android.permission.INTERNET">
		<activity android:name=".activities.FeedList">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>


	</application>
	
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	
</manifest>